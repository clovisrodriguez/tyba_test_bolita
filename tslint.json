{
  "defaultSeverity": "error",
  "extends": ["tslint-react"],
  "rules": {
    "jsx-no-multiline-js": false,
    "member-access": false,
    "ordered-imports": false,
    "quotemark": false,
    "no-console": false,
    "semicolon": false,
    "jsx-no-lambda": false,
    "jsx-alignment": false
  },
  "rulesDirectory": [],
  "jsRules": {
    "max-line-lenght": {
      "options": [120]
    }
  },
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  }
}
